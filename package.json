  "dependencies": {
    "gatsby": "^1.9.194",
    "gatsby-image": "^1.0.36",
    "gatsby-link": "^1.6.37",
    "gatsby-plugin-google-analytics": "^1.0.28",
    "gatsby-plugin-netlify": "^1.0.18",
    "gatsby-plugin-netlify-cms": "^1.0.7",
    "gatsby-plugin-react-helmet": "^2.0.5",
    "gatsby-plugin-react-next": "^1.0.9",
    "gatsby-plugin-sharp": "^1.6.28",
    "gatsby-plugin-styled-components": "^2.0.6",
    "gatsby-remark-copy-linked-files": "^1.5.27",
    "gatsby-remark-images": "^1.5.44",
    "gatsby-remark-responsive-iframe": "^1.4.17",
    "gatsby-remark-smartypants": "^1.4.11",
    "gatsby-source-filesystem": "^1.5.19",
    "gatsby-transformer-remark": "^1.7.32",
    "gatsby-transformer-sharp": "^1.6.19",
    "intersection-observer": "^0.5.0",
    "netlify-cms": "1.3.5",
    "normalize.css": "^8.0.0",
    "react-headroom": "^2.2.2",
    "react-helmet": "^5.2.0",
    "react-load-script": "^0.0.6",
    "s3-deploy": "^0.10.0",
    "serve": "^6.5.3",
    "styled-components": "^3.1.6"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "develop-hosted": "gatsby develop --host Hunters-Macbook-Pro.local",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.js\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "deploy-aws": "gatsby build --prefix-paths && s3-deploy './public/**' --cwd './public/' --bucket the-next-website --region us-east-1 --deleteRemoved --gzip",
    "s3-deploy": "s3-deploy './public/**' --cwd './public/' --bucket the-next-website --region us-east-1 --deleteRemoved --gzip",
    "start": "serve public/"
    
  },
  "devDependencies": {
    "gh-pages": "^1.1.0",
    "prettier": "^1.9.2"
  }
}
